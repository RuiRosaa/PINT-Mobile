<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#39BA23" /> <!-- Red color -->
    <size android:width="24dp" android:height="24dp" /> <!-- Adjust size as needed -->
    <corners android:radius="12dp" /> <!-- To create a circular shape -->
    <stroke
        android:width="2dp"
        android:color="#FFFFFF" />
    <item android:top="4dp" android:bottom="4dp" android:left="12dp" android:right="12dp">
        <shape>
            <solid android:color="#FFFFFF" /> <!-- White color for the antenna part -->
            <size android:width="8dp" android:height="16dp" /> <!-- Adjust size as needed -->
            <corners android:radius="4dp" /> <!-- To create rounded corners -->
        </shape>
    </item>
</shape>